{
  "name": "micro-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "home": "ng serve home --configuration development --port 4400 --open",
    "carousel": "ng serve carousel --configuration development --port 4200 --open",
    "login": "ng serve login --configuration development --port 4201 --open",
    "listpage": "ng serve listpage --configuration development --port 4202 --open",
    "shell": "ng serve shell --configuration development --port 4000 --open",
    "start": "concurrently \"npm run home\" \"npm run carousel\" \"npm run login\" \"npm run listpage\" \"npm run shell\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^18.0.6",
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "bootstrap": "^5.3.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.11",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-build-plus": "^18.0.0",
    "typescript": "~5.2.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.1.4"
  }
}
